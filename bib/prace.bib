  keywords = {ferh,mld},
  annotation = {Accepted: 2017-05-26T11:54:59Z},
  file = {/data/references/storage/VHECZHEC/Brajer - 2015 - Laserová spektroskopie materiálů pro spintroniku.pdf;/data/references/storage/4AL2L4NI/62628.html}
}

@phdthesis{brajerStudiumNovychMaterialu2012,
  type = {Bakal\'a\v{r}sk\'a Pr\'ace},
  title = {Studium Nov\'ych Materi\'al\r{u} pro Generov\'an\'i Druh\'e Harmonick\'e Frekvence},
  author = {Brajer, Martin},
  year = {2012},
  month = jun,
  address = {{Praha}},
  abstract = {V t\'eto bakal\'a\v{r}sk\'e pr\'aci se zab\'yv\'ame studiem optick\'ych vlastnost\'i materi\'al\r{u} c\'ilen\v{e} p\v{r}ipravovan\'ych pro \'u\v{c}inn\'e generov\'an\'i druh\'e harmonick\'e frekven- ce (SHG) pro pou\v{z}it\'i v optoelektronice. Konkr\'etn\v{e} jde o m\v{e}\v{r}en\'i prvn\'i hyperpo- larizibility. Vyu\v{z}\'ivat budeme nekoherentn\'iho jevu, hyper-Rayleighova rozptylu. Vzhledem k nekoherentnosti tohoto jevu je efekt velmi slab\'y. Je t\v{r}eba pou\v{z}\'it siln\'y zdroj sv\v{e}tla, pulzn\'i laser a citliv\'e detek\v{c}n\'i za\v{r}\'izen\'i, foton\'asobi\v{c}. Vzorky se p\v{r}ipravuj\'i ve spolupracuj\'ic\'ich chemick\'ych laborato\v{r}\'ich P\v{r}\'irodov\v{e}deck\'e fakul- ty Univerzity Karlovy v Praze. D\'ale n\'as bude zaj\'imat zastoupen\'i dvoufotonov\'e fluorescence ve v\'ysledn\'e intenzit\v{e}, kv\r{u}li zam\v{e}nitelnosti s SHG.},
  school = {Univerzita Karlova},
  annotation = {Accepted: 2017-05-07T18:20:24Z},
  file = {/data/references/storage/AXZJHMIZ/Brajer - 2012 - Studium nových materiálů pro generování druhé harm.pdf}
}

