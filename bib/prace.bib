  year = {2017},
  month = jun,
  address = {{Praha}},
  abstract = {Abstrakt:V r\'amci tejto pr\'ace boli sk\'uman\'e ve\v{l}kos\v{t} a smer magnetick\'eho po\v{l}a generovan\'eho prototypom dvojdimenzion\'alneho elektromagnetu, ktor\'y bol ne- d\'avno skon\v{s}truovan\'y v Laborat\'oriu OptoSpintroniky. Tento elektromagnet je zlo\v{z}en\'y z dvoch p\'arov cievok s navz\'ajom kolm\'ymi p\'olov\'ymi nadstavcami. Pre- dov\v{s}etk\'ym sme zistili, \v{z}e spr\'avnym nastaven\'im pr\'udu prech\'adzaj\'uceho cievkami je mo\v{z}n\'e reprodukovate\v{l}ne rotova\v{t} smer po\v{l}a v rovine p\'olov\'ych nadstavcov s ve- \v{l}kos\v{t}ou po\v{l}a pribli\v{z}ne 200 mT. Us\'udili sme tie\v{z}, \v{z}e generovan\'e pole je pomerne homog\'enne v oblasti s polomerom pribli\v{z}ne 5 mm, \v{c}o odpoved\'a rozmerom typic- kej vzorky. Na druh\'u druh\'u stranu, funk\v{c}nos\v{t} elektromagnetu v oblasti ni\v{z}\v{s}\'ich pol\'i nemohla by\v{t} otestovan\'a jednozna\v{c}ne kv\^oli technick\'ym probl\'emom s nami vyrobenou Hallovskou mierkou. Tento probl\'em bude rie\v{s}en\'y v nasleduj\'ucej pr\'aci. 1},
  school = {Univerzita Karlova},
  keywords = {magnet,mol},
  annotation = {Accepted: 2021-03-25T18:51:45Z},
  file = {/data/references/storage/B9JER9QB/Kimák - 2017 - Charakterizácia dvojdimenzionálneho elektromagnetu.pdf;/data/references/storage/8X5F2A4Q/86374.html}
}

@phdthesis{kimakOptickaSpektroskopieAntiferomagnetu2019,
  type = {Diplomov\'a Pr\'ace},
  title = {Optick\'a Spektroskopie Antiferomagnet\r{u}},
  author = {Kim{\'a}k, Jozef},
  year = {2019},
  month = jun,
  address = {{Praha}},
  abstract = {Antiferomagnety, magneticky usporiadan\'e materi\'aly bez makroskopickej magnetiz\'acie, je pomerne \v{t}a\v{z}k\'e \v{s}tudova\v{t} be\v{z}n\'ymi n\'astrojmi magnetickej charakteriz\'acie. T\'ato diplomov\'a pr\'aca bola zameran\'a na ich optick\'y v\'yskum. Podrobne sme sk\'umali magnetooptick\'e efekty kvadratick\'e v magnetiz\'aci\'i, magnetick\'y line\'arny dichroizmus a Voigtov jav, ktor\'e s\'u pr\'itomn\'e aj v antiferomagnetoch. Presk\'umali sme magnetick\'y polovodi\v{c} (Ga,Mn)As, kde s\'u tieto efekty v\'yrazn\'e a vyu\v{z}it\'im dvojdimenzion\'alneho elektromagnetu sme dok\'azali, \v{z}e tieto javy mo\v{z}no vyu\v{z}i\v{t} na stanovenie jeho magnetickej a magnetooptickej anizotropie. Alternat\'ivny pr\'istup bol pou\v{z}it\'y v tenkom filme z nekoline\'arneho antiferomagnetu Mn3NiN, kde sme \v{s}tudovali zmeny optick\'ych a magnetooptick\'ych vlastnost\'i. Zistili sme, \v{z}e s\'u silne ovplyvnen\'e pnut\'im spojen\'ym s teplotne indukovan\'ym f\'azov\'ym prechodom v substr\'ate STO. To demon\v{s}truje, \v{z}e optick\'a spektroskopia m\^o\v{z}e by\v{t} pou\v{z}it\'a na v\'yskum antiferomagnetov pomerne efekt\'ivne.},
  school = {Univerzita Karlova},
  keywords = {magnet,mld,mol},
  annotation = {Accepted: 2019-07-10T14:53:50Z},
  file = {/data/references/storage/YCMA73Z3/Kimák - 2019 - Optická spektroskopie antiferomagnetů.pdf}
}

@phdthesis{kubascikMagnetooptickeStudiumAntiferomagnetickych2019,
  type = {Bakal\'a\v{r}sk\'a Pr\'ace},
  title = {Magneto-Optick\'e \v{S}t\'udium Antiferomagnetick\'ych Materi\'alov Pre Spintroniku},
  author = {Kuba{\v s}{\v c}{\'i}k, Peter},
  year = {2019},
  month = sep,
  address = {{Praha}},
  abstract = {The main goal of this thesis was to develop an experimental technique for investigation of ferromagnetic and antiferromagnetic materials using magneto-optical effects that are quadratic in magnetization. Using a prototype of 2-dimensional electromagnet, which enables to rotate magnetic field of constant magnitude in the sample plane, we were able to study spectral dependences and anisotropies of corresponding magneto-optical coefficients. In ferromagnetic semiconductor GaMnAs we revealed that the anisotropy of Voigt effect magneto-optical coefficient is strongly wavelength dependent - this coefficient can be both isotropic and anisotropic. Very strong anisotropy of this coefficient was observed for metallic FeRh in a ferromagnetic state. Finally, a new method that can be used to measure Voigt effect in antiferromagnetic state of FeRh was demonstrated.},
  school = {Univerzita Karlova},
  keywords = {ferh,mld,mol},
  annotation = {Accepted: 2019-10-18T08:14:32Z},
  file = {/data/references/storage/PWG2YGFL/Kubaščík - 2019 - Magneto-optické štúdium antiferomagnetických mater.pdf;/data/references/storage/LHYJ2EW8/109960.html}
}

@phdthesis{kubascikStudySpintronicPhenomena2021,
  type = {Diplomov\'a Pr\'ace},
  title = {Study of Spintronic Phenomena in Magnetically Ordered Stuctures Using Terahertz Spectroscopy},
  author = {Kuba{\v s}{\v c}{\'i}k, Peter},
  year = {2021},
  month = jun,
  address = {{Praha}},
  abstract = {Hlavn\'ym cie\v{l}om tejto pr\'ace bolo prinies\v{t} prv\'y experiment\'alny d\^okaz o existenci\'i spin-Hall magnetorezistencie (SMR) v Terahertzovej (THz) spektr\'alnej oblasti. Ako hlavn\'u met\'odu sme zvolili \v{c}asovo-rozl\'i\v{s}en\'u THz spektroskopiu (TDTS) a prezentovali sme experiment\'alne usporiadanie, ktor\'e umo\v{z}\v{n}uje pozorova\v{t} efekt\'ivne SMR alebo in\'e magnetorezist\'ivne javy s podobnou symetriou v \v{s}irokom rozsahu THz frekvenci\'i. V r\'amci tejto pr\'ace sme sa zamerali na \v{s}t\'udium dvojvrstiev ferimagnetick\'eho izol\'atoru a \v{t}a\v{z}k\'eho kovu (FI/HM) a kovov\'ych dvojvrstiev feromagnetu CoFeB a \v{t}a\v{z}k\'eho kovu Pt (FM/HM). Zatia\v{l} \v{c}o SMR v FI/HM \v{s}trukt\'ure vykazuje v\'yrazn\'y pokles u\v{z} v n\'izkych THz frekvenci\'ach ({$<$}1 THz), SMR v dvojvrstv\'ach FM/HM pretrv\'ava aj nad 30 THz. Tieto v\'ysledky s\'u nasledovne vysvetlen\'e rozdielnymi mechanizmami SMR. Druh\'a \v{c}as\v{t} tejto pr\'ace sa zaober\'a kon\v{s}trukciou nov\'eho TDTS experiment\'alneho usporiadania a jednoduch\'ym modelom THz experiment\'alneho usporiadania pomocou Gaussovsk\'ych zv\"azkov THz \v{z}iarenia. V\'ysledky simul\'aci\'i boli n\'asledne porovnan\'e s pr\'islu\v{s}n\'ymi experiment\'alnymi meraniami pomocou spintronick\'eho THz emitoru (STE). Posledn\'a \v{c}as\v{t} sa venuje popisu intenz\'ivnym laserov\'ych pulzov z plo\v{s}n\'ych STE.},
  school = {Univerzita Karlova},
  annotation = {Accepted: 2021-07-20T09:07:12Z},
  file = {/data/references/storage/WZ4DPUWB/Kubaščík - 2021 - Study of spintronic phenomena in magnetically orde.pdf}
}

